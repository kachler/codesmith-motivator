{
    "manifest_version": 2,
  
    "name": "Codesmith Motivator",
    "description": "If you're feeling down about coding, click this.",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.png",
     "default_popup": "CMotivator.html"
    },
    "permissions": [
     "activeTab"
     ]
  }