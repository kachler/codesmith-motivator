{
    "manifest_version": 2,
  
    "name": "Codesmith Motivator",
    "description": "If you're feeling down about coding, click this.",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon128.png",
     "default_popup": "CMotivator.html",
     "icons": { "16": "icon16.png",
        "48": "icon48.png",
       "128": "icon128.png" }
    },
    "permissions": [
     "activeTab"
     ]
  }